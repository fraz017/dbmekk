<%= form_for @event, remote: true do |f| %>
  <div class="form-inputs">
    <label>Name</label>
    <%= f.text_field :title, class: "form-control", required: true %>
    <label>Email</label>
    <%= f.email_field :email, class: "form-control", required: true %>
    <label>Phone Number</label>
    <%= f.text_field :phone_number, class: "form-control", required: true %>
    <label>License No</label>
    <%= f.text_field :license_no, class: "form-control", required: true %>
    <label>Description</label>
    <%= f.text_field :description, class: "form-control" %>
    <%= f.hidden_field :start, as: :hidden, value: Date.today, class: 'form-control input-sm start_hidden' %>
    <%= f.hidden_field :end, as: :hidden, value: Date.today, class: 'form-control input-sm end_hidden' %>
  </div>
  <br/>
  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success" %>
  </div>
<% end %>
